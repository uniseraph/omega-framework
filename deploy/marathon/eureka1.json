{
  "id": "eureka1",
  "cpus": 0.25,
  "mem": 384,
  "disk": 0,
  "instances": 1,
  "container": {
    "docker": {
      "image": "registry.cn-hangzhou.aliyuncs.com/omega-reg/omega-framework-eureka:0.1",
      "network": "BRIDGE",
      "forcePullImage": false,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp",
          "name": null,
          "labels": null
        }
      ]
    },
    "type": "DOCKER"
  },
  "env": {
    "JAVA_OPTS": "-Xms256m -Xmx256m" ,
    "server.port": "8080" ,
    "spring.profiles.active": "eureka1",
    "eureka1.instance.hostname": "eureka1.service.consul" ,
    "eureka2.instance.hostname": "eureka2.service.consul" ,
    "eureka3.instance.hostname": "eureka3.service.consul"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/health",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}

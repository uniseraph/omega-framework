{
  "id": "/configserver",
  "cmd": "sh -x start-configserver.sh --server.port=8080 --spring.rabbitmq.host=rabbitmq.mesos.consul --euerka.client.serviceUrl.defaultZone=http://eureka1.mesos.consul:80/eureka,http://eureka2.mesos.consul:80/eureka,http://eureka3.mesos.consul:80/eureka --logging.level.root=info",
  "cpus": 0.5,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "registry.cn-hangzhou.aliyuncs.com/omega-reg/omega-framework:0.1",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10003,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "path": "/health",
      "protocol": "HTTP",
      "ignoreHttp1xx": false
    }
  ],
  "portDefinitions": [
    {
      "port": 10003,
      "protocol": "tcp",
      "name": "default",
      "labels": {}
    }
  ]
}